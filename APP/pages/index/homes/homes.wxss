/*排行榜结束*/
.footer{
  width:100%;
  background:#f1f6f4;
  padding:55rpx 0;
}
.row{
  width: 100%;
}
.row .header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 38rpx;
}
.row .header .ranking{
  font-size: 32rpx;
  margin-left: 45rpx;
}
.row .header .volume text{
  font-size: 22rpx;
  padding-bottom: 8rpx;
}
.row .header .volume text.active{
  border-bottom: 4rpx solid #000;
}
.row .header .volume text:nth-of-type(2){
  margin-left: 48rpx;
  margin-right: 45rpx;
}
.row .body{
  display: flex;
  justify-content: center;
  align-items: center; 
}
.row .body .center{
  width: 231rpx;
  height: 292rpx;
  margin-right: 19rpx;
  background: #fff;
  border-radius: 8rpx;
}
.row .body .center:last-child{
  margin-right: 0;
}
.row .body .center image{
  width: 231rpx;
  height: 150rpx;
}
.row .body .center .circle{
  display: flex;
  justify-content: center;
  margin-top: -30rpx;
  margin-bottom: 20rpx;
}
.row .body .center .circle text{
  width: 48rpx;
  height: 48rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  background: #0e8b4c;
  border-radius: 50%;
}
.row .body .center .text{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.row .body .center .text text{
  font-size: 19rpx; 
  margin-bottom: 10rpx;
}
.row .body .center .text .person{
  font-size: 24rpx;
  color: #ccc;
}

/*排行榜结束*/
/* 商品列表开始 */

.goodList{
  display: flex;
  box-sizing: border-box;
  width: 100%;
