  box-shadow: 9px -1px 7px 2px rgba(122, 122, 122, 0.11);
}
.addgoods .addgoodsBox .marque .Price{
  color:#d60e15;
  margin-top: 37rpx;
}
.addgoods .addgoodsBox .marque .num{
