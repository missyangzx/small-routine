}
.addgoods .addgoodsBox .marque .close{
  font-size: 50rpx;
  margin-right: 25rpx;
  margin-top: 15rpx;
}
.addgoods .addgoodsBox .marque view{
  margin: 0;
  padding: 0;
}
.addgoods .addgoodsBox .marque .addGoodsImg{
